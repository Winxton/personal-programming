import json
import traceback

def main():
	tweet_file = open ("output.txt")
	freqMap = {}
	int tot = 0

	for i in range (0,5):
		try:	
			line = tweet_file.readline()
			data = json.loads(line)
			text = data["text"]
			tokens = text.split()
			for token in tokens:
				tot += 1
				if token in freqMap:
					freqMap[token] += 1
				else :
					freqMap[token] = 1
		except:
			traceback.print_exc()
	print tot

main()
